{
    "name": "kseo/seo-booster",
    "description": "Full-featured, AI-powered WordPress SEO plugin with OpenAI integration",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Krish Yadav",
            "email": "krish@keseobooster.com",
            "homepage": "https://github.com/portrara"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "KSEO\\SEO_Booster\\": "inc/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "phpcs": "phpcs --standard=phpcs.xml.dist",
        "phpcbf": "phpcbf --standard=phpcs.xml.dist",
        "lint": "find . -name \"*.php\" -not -path \"./vendor/*\" -exec php -l {} \\;"
    }
} 